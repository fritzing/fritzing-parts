<?xml version='1.0' encoding='UTF-8' standalone='no'?>
<module moduleId="multimeter_v_dc_1" fritzingVersion="0.9.4" referenceFile="multimeter_v_dc_1.fzp">
  <version>4</version>
  <author>Andres Faina (modified by vanepp Nov 2020)</author>
  <title>Multimeter (DC Voltage)</title>
  <label>VM</label>
  <date>fr okt 30 2020</date>
  <tags>
    <tag>Multimeter</tag>
  </tags>
  <properties>
    <property name="family">Multimeter</property>
    <property name="variant">Voltmeter (DC)</property>
  </properties>
  <description>&lt;!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0//EN" "http://www.w3.org/TR/REC-html40/strict.dtd"&gt;
&lt;html&gt;&lt;head&gt;&lt;meta name="qrichtext" content="1" /&gt;&lt;style type="text/css"&gt;
p, li { white-space: pre-wrap; }
&lt;/style&gt;&lt;/head&gt;&lt;body style=" font-family:'MS Shell Dlg 2'; font-size:8.25pt; font-weight:400; font-style:normal;"&gt;
&lt;p style=" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;"&gt;A standard multimeter to measure volatages, currents and resistances&lt;/p&gt;&lt;/body&gt;&lt;/html&gt;</description>
  <spice>
    <line>* Multimeter, com probe connected to {net connector0} and voltage probe connected to {net connector1}</line>
  </spice>
  <views>
    <iconView>
      <layers image="breadboard/multimeter_v_dc_1_breadboard.svg">
        <layer layerId="icon"/>
      </layers>
    </iconView>
    <breadboardView>
      <layers image="breadboard/multimeter_v_dc_1_breadboard.svg">
        <layer layerId="breadboard"/>
      </layers>
    </breadboardView>
    <schematicView>
      <layers image="schematic/multimeter_v_dc_1_schematic.svg">
        <layer layerId="schematic"/>
      </layers>
    </schematicView>
    <pcbView>
      <layers image="pcb/multimeter_v_dc_1_pcb.svg">
        <layer layerId="partimage"/>
      </layers>
    </pcbView>
  </views>
  <connectors>
    <connector name="COM Probe" type="female" id="connector0">
      <description>COM</description>
      <views>
        <breadboardView>
          <p svgId="connector0pin" layer="breadboard"/>
        </breadboardView>
        <schematicView>
          <p svgId="connector0pin" terminalId="connector0terminal" layer="schematic"/>
        </schematicView>
        <pcbView>
          <p svgId="connector0pin" layer="partimage"/>
        </pcbView>
      </views>
    </connector>
    <connector name="V Probe" type="female" id="connector1">
      <description>Voltage probe</description>
      <views>
        <breadboardView>
          <p svgId="connector1pin" layer="breadboard"/>
        </breadboardView>
        <schematicView>
          <p svgId="connector1pin" terminalId="connector1terminal" layer="schematic"/>
        </schematicView>
        <pcbView>
          <p svgId="connector1pin" layer="partimage"/>
        </pcbView>
      </views>
    </connector>
    <connector name="A Probe" type="female" id="connector2">
      <description>Current probe</description>
      <views>
        <breadboardView>
          <p svgId="connector2pin" layer="breadboard"/>
        </breadboardView>
        <schematicView>
          <p svgId="connector2pin" terminalId="connector2terminal" layer="schematic" hybrid="yes"/>
        </schematicView>
        <pcbView>
          <p svgId="connector2pin" layer="partimage"/>
        </pcbView>
      </views>
    </connector>
  </connectors>
</module>
