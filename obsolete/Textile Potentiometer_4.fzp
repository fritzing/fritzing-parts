   <layers image="icon/Textile_potentiometer.svg">
    <layer layerId="icon"/>
   </layers>
  </iconView>
  <schematicView>
   <layers image="schematic/basic_poti.svg">
    <layer layerId="schematic"/>
   </layers>
  </schematicView>
  <pcbView>
   <layers image="pcb/jumper_3_100mil.svg">
    <layer layerId="copper0"/>
    <layer layerId="silkscreen"/>
    <layer layerId="keepout"/>
    <layer layerId="soldermask"/>
    <layer layerId="outline"/>
    <layer layerId="copper1"/>
   </layers>
  </pcbView>
  <breadboardView>
   <layers image="breadboard/Textile_potentiometer.svg">
    <layer layerId="breadboard"/>
   </layers>
  </breadboardView>
 </views>
